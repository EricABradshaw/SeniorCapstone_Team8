    "cookie-parser": "~1.4.4",
    "cors": "2.8.5",
    "debug": "~2.6.9",
    "dotenv": "16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "http-errors": "~1.6.3",
    "mocha": "10.2.0",
    "mongoose": "8.0.0",
    "morgan": "~1.9.1",
    "react": "18.2.0",
    "sass": "^1.69.5"
  }
}
